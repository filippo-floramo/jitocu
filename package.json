{
  "name": "jitocu",
  "version": "0.1.1",
  "author": {
    "name": "Filippo FLoramo",
    "email": "filifloramo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filippo-floramo/jitocu"
  },
  "description": "Copy Jira issues to ClickUp with an interactive terminal UI",
  "type": "module",
  "bin": {
    "jitocu": "./dist/cli.js"
  },
  "scripts": {
    "start": "bun run src/cli.ts",
    "dev": "bun --watch src/cli.ts",
    "build": "bun build ./src/cli.ts --outdir ./dist --target node --banner '#!/usr/bin/env node' ",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "jira",
    "clickup",
    "cli",
    "terminal",
    "bun",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "fuzzy": "^0.1.3",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.9.3"
  }
}
